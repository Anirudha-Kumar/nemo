focus [lindex $keyList 0]

foreach w $keyList {
    if {1} {
       bind $w <Return> {tab $keyList}
       $w insert end {}
    }
}

proc tab list {
    set i [lsearch -exact $list [focus]]
    incr i
    if {$i >= [llength $list]} {
        set i 0
    }
    focus [lindex $list $i]
}

