#
#   this Makefile.nemo is normally included by the master Makefile
#   at the top level and adds the target to merge needed object
#   code into NEMO.
#   

BINS = addgravity getgravity gyrfalcON lagrange_radii \
        manipulate mkdehnen mkking mkplum scale_eps snapstack \
 	symmetrize TestGrav

nemo_bins: utils $(BINS)
	(cd utils; make)
	make 
	(cd $(HOS); cp $(BINS) $(NEMOBIN))
	cp $(LIB)/lib*.so $(NEMOLIB)
